{
  "name": "gatekeeper",
  "version": "0.1.0",
  "private": true,
  "description": "Ephemeral database session management with time-limited access",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "dev": "turbo run dev --parallel",
    "clean": "turbo run clean && rm -rf node_modules",
    "gen:openapi": "turbo run gen:openapi",
    "agent:dev": "PGSSLMODE=disable pnpm --filter agent dev",
    "agent:health": "PGSSLMODE=disable pnpm --filter cli start agent health",
    "agent:test": "PGSSLMODE=disable pnpm --filter cli start agent test",
    "session:create": "PGSSLMODE=disable pnpm --filter cli start session create",
    "session:list": "PGSSLMODE=disable pnpm --filter cli start session list",
    "users:init": "PGSSLMODE=disable pnpm --filter user-mgmt start init",
    "users:create": "PGSSLMODE=disable pnpm --filter user-mgmt start create",
    "users:list": "PGSSLMODE=disable pnpm --filter user-mgmt start list",
    "users:show": "PGSSLMODE=disable pnpm --filter user-mgmt start show",
    "users:interactive": "PGSSLMODE=disable pnpm --filter user-mgmt start interactive"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "ts-standard": "^12.0.2",
    "turbo": "^1.12.0",
    "typescript": "^5.3.0"
  },
  "pnpm": {
    "overrides": {
      "@types/node": "^20.11.0"
    }
  }
}