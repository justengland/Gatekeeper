{
  "name": "@gatekeeper/agent",
  "version": "0.1.0",
  "description": "Gatekeeper Agent - Ephemeral database session management service",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gatekeeper-agent": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/server.ts",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build:lambda": "npm run build && npm run package:lambda",
    "package:lambda": "node scripts/package-lambda.js",
    "deploy:localstack": "node scripts/deploy-localstack.js",
    "test:lambda": "node scripts/test-lambda.js",
    "lint": "ts-standard",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@gatekeeper/shared": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^1.6.1",
    "archiver": "^6.0.1",
    "aws-sdk": "^2.1498.0",
    "supertest": "^6.3.4",
    "testcontainers": "^10.5.0",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gatekeeper",
    "database",
    "ephemeral",
    "sessions",
    "postgresql",
    "lambda",
    "security"
  ]
}