{
  "name": "@gatekeeper/user-mgmt",
  "version": "0.1.0",
  "description": "Gatekeeper User Management CLI - Add and manage database users with the agent stack",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gk-users": "dist/cli.js",
    "gatekeeper-users": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "ts-standard 'src/**/*.ts'",
    "lint:fix": "ts-standard 'src/**/*.ts' --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@gatekeeper/shared": "workspace:*",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "table": "^6.8.1",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/inquirer": "^9.0.7",
    "@types/uuid": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2",
    "vitest": "^1.6.1",
    "@vitest/coverage-v8": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gatekeeper",
    "user-management",
    "cli",
    "database",
    "users",
    "postgresql",
    "permissions"
  ]
}